<div class="row">
{% for member in data.members %}

<div class="col-sm-4 col-lg-3">
    <div class="card" style="width: 18rem;">
  <img src="img/members/{{ member.image }}" class="card-img-top img fluid" alt="...">
  <div class="card-body">
    <h5 class="card-title text-center">{{ member.name }}</h5>
    <p class="card-text text-center"> {{member.instruments}} </p>
    <a href="{{ member.slug }}.html" class="btn btn-primary">About</a>
  </div>
</div>
</div>
{% endfor %}
</div>